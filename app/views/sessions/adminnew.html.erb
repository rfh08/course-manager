<% provide(:title, "Admin Log in") %>
<h1>Admin Log in</h1>

      
 <ul class="nav nav-tabs">
    <li> <%= link_to "Staff", login_path %> </li>
    <li> <%= link_to "Admin", adminlogin_path %> </li>
</ul>
     
     <br>
     
<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(:session, url: adminlogin_path) do |f| %>

      <%= f.label "username"%>
      <%= f.text_field :email %>

      <%= f.label :password %>
      <%= f.password_field :password %>

      <%= f.submit "Log in", class: "btn btn-success" %>
    <% end %>

  </div>
</div>